				"\\usepackage{amssymb}",
				"\\usepackage{relsize}",
				"\\usepackage[none]{hyphenat}",
				"\\usepackage{graphicx}",
				" ",
				"\\title{title$1}",
				"\\author{author}",
				"\\date{date}",
				"\\begin{document}",
