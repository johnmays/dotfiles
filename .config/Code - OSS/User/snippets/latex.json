{
	"TeX Matrix (johnmays)" :{
		"prefix": "mat",
		"body": ["\\begin{matrix}",
				"\tA$1 & B\\\\\\",
				"\tC & D",
				"\\end{matrix}"],
		"description": "TEX Mathmode Matrix"
	},

	"TeX Tabular (johnmays)" :{
		"prefix": "tab",
		"body": ["\\begin{tabular}{|c|c|$1}",
				"\t\\hline",
				"\tA & B\\\\\\",
				"\t\\hline",
				"\tC & D\\\\\\",
				"\t\\hline",
				"\\end{tabular}"],
		"description": "TEX table"
	},

	"TeX Image (johnmays)" : {
		"prefix": "im",
		"body": "\\includegraphics[scale=0.5]{path$1}",
		"description": "TeX image display"
	},

	"TeX Figure (johnmays)":{
		"prefix": "fig",
		"body": [
				"\\begin{figure}[h]",
				"\t$1",
				"\t\\caption{cap}",
				"\\end{figure}"
		],
		"description": "TeX Figure Skeleton"
	},

	"Document Template (johnmays)" :{
		"prefix": "begin",
		"body": ["\\documentclass{article}",
				"\\usepackage[utf8]{inputenc}",
				"\\usepackage[letterpaper, total={6.5in, 9in}]{geometry}",
